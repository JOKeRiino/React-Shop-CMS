{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "collections": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::collection.collection",
      "inversedBy": "products"
    },
    "price": {
      "type": "decimal"
    },
    "strike_price": {
      "type": "decimal"
    },
    "variant": {
      "type": "component",
      "repeatable": false,
      "component": "variant.variant"
    },
    "Washing": {
      "type": "component",
      "repeatable": false,
      "component": "metadata.metadata"
    },
    "Material": {
      "type": "component",
      "repeatable": false,
      "component": "metadata.metadata"
    },
    "Fit": {
      "type": "component",
      "repeatable": false,
      "component": "metadata.metadata"
    },
    "price_id": {
      "type": "string"
    }
  }
}
